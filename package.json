    "@angular/platform-browser-dynamic": "2.2.4",
    "@angular/router": "3.2.4",
    "@ng-bootstrap/ng-bootstrap": "1.0.0-alpha.14",
    "bootstrap": "4.0.0-alpha.5",
    "chart.js": "2.4.0",
    "core-js": "2.4.1",
    "font-awesome": "4.7.0",
    "moment": "2.17.1",
    "ng2-charts": "1.4.4",
    "reflect-metadata": "0.1.8",
    "rxjs": "5.0.0-beta.12",
    "zone.js": "0.6.26"
  },
